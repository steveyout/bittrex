{
  "name": "backend",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "clean": "node -e \"const fs=require('fs');const p=require('path');const d=p.join(__dirname,'dist');if(fs.existsSync(d)){fs.rmSync(d,{recursive:true,force:true});console.log('Cleaned dist folder')}\"",
    "build": "tsc -p tsconfig.json --noEmit false && node scripts/minify.mjs",
    "build:nominify": "tsc -p tsconfig.json --noEmit false",
    "bundle": "npm run clean && tsc -p tsconfig.json --noEmit false && node scripts/minify.mjs",
    "start": "pm2 start dist/index.js --env production",
    "seed": "sequelize-cli db:seed:all --config ./config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPatterns=integration",
    "test:unit": "jest --testPathPatterns=unit",
    "test:binary": "jest --testPathPatterns=binary",
    "test:exchange": "jest --testPathPatterns=exchange",
    "lint": "eslint src --ext .ts",
